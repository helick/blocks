{
    "name": "helick/blocks",
    "type": "library",
    "description": "The Helick blocks",
    "keywords": [
        "helick",
        "wordpress",
        "gutenberg",
        "blocks"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/helick/blocks/issues",
        "source": "https://github.com/helick/blocks"
    },
    "authors": [
        {
            "name": "Evgenii Nasyrov",
            "email": "evgenii@helick.io"
        }
    ],
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.6",
        "helick/contracts": "^1.1",
        "htmlburger/carbon-fields": "^3.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Helick\\Blocks\\": "src/"
        }
    },
    "scripts": {
        "lint": [
            "phpcs -p --standard=psr2 src"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
